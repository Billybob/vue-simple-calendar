{
	"name": "vue-simple-calendar",
	"version": "6.0.0-b2",
	"private": false,
	"description": "Simple calendar event control",
	"author": "richardtallent <richard@tallent.us>",
	"scripts": {
		"dev": "vite",
		"devs": "vite --https",
		"builddx": "vuedx-typecheck . && vite build",
		"build": "vite build"
	},
	"files": [
		"dist"
	],
	"main": "./dist/vue-simple-calendar.umd.js",
	"module": "./dist/vue-simple-calendar.es.js",
	"exports": {
		".": {
			"import": "./dist/vue-simple-calendar.es.js",
			"require": "./dist/vue-simple-calendar.umd.js"
		}
	},
	"dependencies": {
		"vue": "^3.0.5"
	},
	"devDependencies": {
		"@typescript-eslint/parser": "^4.14.0",
		"@vitejs/plugin-vue": "^1.1.2",
		"@vue/compiler-sfc": "^3.0.5",
		"@vuedx/typecheck": "^0.4.1",
		"@vuedx/typescript-plugin-vue": "^0.4.1",
		"autoprefixer": "^10.2.3",
		"eslint": "^7.18.0",
		"eslint-config-tabsanity": "^2.0.0",
		"eslint-plugin-prettier": "^3.3.1",
		"eslint-plugin-vue": "^7.5.0",
		"postcss": "^8.2.4",
		"prettier": "^2.2.1",
		"rollup-plugin-analyzer": "^4.0.0",
		"stylelint": "^13.9.0",
		"stylelint-config-standard": "^20.0.0",
		"typescript": "^4.1.3",
		"vite": "^2.0.0-beta.46"
	},
	"eslintConfig": {
		"extends": [
			"plugin:vue/vue3-essential",
			"eslint:recommended",
			"@vue/prettier",
			"tabsanity"
		],
		"parserOptions": {
			"parser": "@typescript-eslint/parser"
		}
	},
	"prettier": {
		"useTabs": true,
		"semi": false,
		"singleQuote": false,
		"bracketSpacing": true,
		"trailingComma": "es5",
		"printWidth": 180
	},
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"defaults",
		"not IE 11",
		"maintained node versions"
	],
	"license": "MIT",
	"repository": "https://github.com/richardtallent/vue-simple-calendar",
	"stylelint": {
		"extends": "stylelint-config-standard",
		"exclude": [
			"dist"
		],
		"rules": {
			"indentation": "tab",
			"declaration-block-trailing-semicolon": null,
			"no-descending-specificity": null
		}
	}
}
